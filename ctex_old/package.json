{
  "name": "ctex",
  "displayName": "ctex",
  "description": "ctex language highlighter",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "ctex",
        "aliases": [
          "ctex",
          "ctex"
        ],
        "extensions": [
          ".ctex"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ctex",
        "scopeName": "source.ctex",
        "path": "./syntaxes/ctex.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mathjax": "0.0.36",
    "@types/node": "^18",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^7.14.0",
    "@typescript-eslint/parser": "^7.14.0",
    "eslint": "^8.26.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "datauri": "^4.1.0",
    "mathjax-full": "^3.2.2"
  }
}
